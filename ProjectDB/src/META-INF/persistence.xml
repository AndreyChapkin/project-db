	<persistence-unit name="ProjectDB" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>entities.TechObject</class>
		<class>entities.User</class>
		<class>entities.Message</class>
		<class>entities.Document</class>
		<class>entities.Product</class>
		<class>entities.TechFile</class>
		<class>entities.MeasureUnit</class>
		<class>entities.ProductGroup</class>
		<class>entities.TechProcess</class>
		<class>entities.TypicBlank</class>
		<class>entities.TypicOperation</class>
		<class>entities.BlankGroup</class>
		<class>entities.Material</class>
		<class>entities.MaterialGroup</class>
		<class>entities.TechOperation</class>
		<class>entities.OperationGroup</class>
		<class>entities.Profession</class>
		<class>entities.TimeNorm</class>
		<class>entities.Equipment</class>
		<class>entities.EquipmentGroup</class>
		<class>entities.MechDegree</class>
		<class>entities.WorkCondition</class>
		<class>entities.Tool</class>
		<class>entities.ToolGroup</class>
		<class>entities.TechTransit</class>
		<class>entities.ParamControl</class>
		<class>entities.TechControl</class>
		<properties>
			<property name="eclipselink.ddl-generation" value="create-tables"/>
			<property name="eclipselink.logging.level" value="FINE"/>
		</properties>
	</persistence-unit>
</persistence>
